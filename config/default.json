{
    "figsize": [12, 10],
    "ylabel_fontsize" : 24,
    "xlabel_fontsize" : 24,
    "cms_label" : "Work in Progress",
    "ratiopad" : {
        "height" : 0.5,
        "hspace" : 0.0,
        "auto_ylim" : {
            "padding" : 0.15,
            "percentile" : 97.5,
            "min_threshold" : 0.2
        }
    },
    "ylim_tweak" : {
        "perversity_threshold" : 1000,
        "padding_factor" : 10
    },
    "fancy_prebinned_labels" : {
        "enabled" : true,
        "max_ndim" : 3,
        "ticksize" : 30,
        "fontsize" : 14,
        "max_minor_ticks" : 100,
        "check_label_overlap" : true,
        "min_fontsize" : 8
    },
    "axis_labels": {
        "pt" : "$p_{T}$ [GeV]",
        "eta" : "$\\eta$",
        "phi" : "$\\phi$",
        "r" : "$r$"
    },
    "auto_logx_patterns" : [
        "*pt",
        "*MET",
        "*HT"
    ]
}